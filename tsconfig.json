{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "@lb/*": ["./src/lib/*"],
      "@c/*": ["./src/components/*"],
      "@p/*": ["./src/pages/*"],
      "@l/*": ["./src/layouts/*"],
    },
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}